{
  "name": "learning-docker",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir ./dist --target=bun", 
    "docker:start": " bunx --bun prisma migrate dev && bunx --bun prisma generate && bun dist/index.js", 
    "dev": "bun src/index.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/express": "^5.0.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "prisma": "^7.2.0"
  }
}
